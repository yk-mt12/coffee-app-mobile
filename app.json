{
  "expo": {
    "name": "coffee-app",
    "slug": "coffee-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/coffee_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {},
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/coffee_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/images/coffee_icon.png"
    },
    "description": "ドリップ珈琲のためのアプリケーション。\n抽出量 or 豆量を入力することで比率に適した量が表示されます。",
    "githubUrl": "https://github.com/yk-mt12/coffee-app-mobile"
  }
}
