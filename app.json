{
  "expo": {
    "name": "coffee-app",
    "slug": "coffee-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/coffee_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/coffee_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/coffee_icon.png"
    }
  }
}
